{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    ["@babel/plugin-proposal-private-property-in-object", { "loose": true }]
  ],
  "transformIgnorePatterns": ["/node_modules/(?!${esModules})"]
}
